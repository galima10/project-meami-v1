{
  "expo": {
    "name": "Meami",
    "slug": "meami",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "meami",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.meami.app",
      "versionCode": 1,
      "icon": "./assets/images/precharged/logos/adaptive-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/precharged/logos/adaptive-icon.png",
        "backgroundColor": "#491B1E"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/precharged/logos/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/precharged/logos/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#E09873"
        }
      ],
      "expo-sqlite",
      [
        "expo-font",
        {
          "android": {
            "fonts": [
              {
                "fontFamily": "SN",
                "fontDefinitions": [
                  { "path": "./assets/fonts/SN-Regular.ttf", "weight": 400 },
                  { "path": "./assets/fonts/SN-Medium.ttf", "weight": 500 },
                  { "path": "./assets/fonts/SN-SemiBold.ttf", "weight": 600 },
                  { "path": "./assets/fonts/SN-Bold.ttf", "weight": 700 }
                ]
              }
            ]
          }
        }
      ],
      [
        "expo-asset",
        {
          "assets": [
            "./assets/images/precharged/logos/logo.png",
            "./assets/images/precharged/logos/logo-text.png",
            "./assets/images/precharged/illustrations/viewImages/storage@3x.jpg",
            "./assets/images/precharged/illustrations/viewImages/recipes@3x.jpg",
            "./assets/images/precharged/illustrations/viewImages/ingredients@3x.jpg",
            "./assets/images/precharged/illustrations/viewImages/cooking@3x.jpg",
            "./assets/images/precharged/illustrations/recipesTypes/vegetables@3x.jpg",
            "./assets/images/precharged/illustrations/recipesTypes/meats@3x.jpg",
            "./assets/images/precharged/illustrations/recipesTypes/desserts@3x.jpg",
            "./assets/images/precharged/illustrations/recipesTypes/accompaniements@3x.jpg",
            "./assets/images/precharged/background/stock@3x.jpg",
            "./assets/images/precharged/background/recipes@3x.jpg",
            "./assets/images/precharged/background/menu@3x.jpg",
            "./assets/images/precharged/background/infos@3x.jpg",
            "./assets/images/precharged/background/cart@3x.jpg"
          ]
        }
      ]
    ],
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/4201e8a9-82b6-4928-84f4-6b2ddf6dfaf9"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4201e8a9-82b6-4928-84f4-6b2ddf6dfaf9"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
