{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "package": "com.meami.app.debug",
        "label": "Meami Debug"
      },
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "distribution": "store",
      "cache": {
        "disabled": false,
        "key": "node-modules",
        "paths": ["node_modules", "package-lock.json"]
      },
      "android": {
        "buildType": "apk",
        "package": "com.meami.app",
        "label": "Meami"
      },
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
